    "test:server": "ts-node-dev -r ./scripts/test-setup.ts --respawn src/test/test-server.ts",
    "test:id-token": "ts-node-dev -r ./scripts/test-setup.ts ./scripts/generate-id-token.ts",
    "test": "jest -c ./jest.unit.json --verbose --runInBand",
